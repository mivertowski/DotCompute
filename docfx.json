{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Core/DotCompute.Abstractions/**.csproj",
            "src/Core/DotCompute.Core/**.csproj",
            "src/Core/DotCompute.Memory/**.csproj",
            "src/Backends/DotCompute.Backends.CPU/**.csproj",
            "src/Backends/DotCompute.Backends.CUDA/**.csproj",
            "src/Backends/DotCompute.Backends.OpenCL/**.csproj",
            "src/Extensions/DotCompute.Algorithms/**.csproj",
            "src/Extensions/DotCompute.Linq/**.csproj",
            "src/Runtime/DotCompute.Runtime/**.csproj",
            "src/Runtime/DotCompute.Generators/**.csproj",
            "src/Runtime/DotCompute.Plugins/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "docs/index.md",
          "docs/toc.yml",
          "docs/articles/**.md",
          "docs/articles/**/toc.yml"
        ]
      },
      {
        "files": [
          "README.md",
          "CHANGELOG.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "modern"
    ],
    "markdownEngineName": "markdig",
    "globalMetadata": {
      "_appName": "DotCompute",
      "_appTitle": "DotCompute - Universal .NET Compute Framework",
      "_enableSearch": true
    }
  }
}
